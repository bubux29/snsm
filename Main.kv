# -*- coding: utf-8 -*-
#: import TabbedPanel kivy.uix.tabbedpanel

<Notebook@TabbedPanel>:
    cols: 1
    spacing: 10
    padding: 20
    do_default_tab: False
    tab_width: self.width / 17 * 3


<Main>:
    notebook: bx.nb
    BoxLayout:
        id: bx
        nb: nb
        size: root.width, root.height
        orientation: 'vertical'
        Notebook:
            id: nb
